:Area rdf:type owl:Class .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#Blizzard

:Blizzard rdf:type owl:Class ;
          
          rdfs:subClassOf :Natural_Disaster .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#Earthquake

:Earthquake rdf:type owl:Class ;
            
            rdfs:subClassOf :Natural_Disaster .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#Flood

:Flood rdf:type owl:Class ;
       
       rdfs:subClassOf :Natural_Disaster .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#High_Risk_Area

:High_Risk_Area rdf:type owl:Class ;
                
                rdfs:subClassOf :Area .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#Hurricane

:Hurricane rdf:type owl:Class ;
           
           rdfs:subClassOf :Natural_Disaster .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#Low_Income_Country

:Low_Income_Country rdf:type owl:Class ;
                    
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :GNI_per_capita ;
                                          owl:maxQualifiedCardinality "1045"^^xsd:nonNegativeInteger ;
                                          owl:onDataRange xsd:integer
                                        ] ;
                    
                    rdfs:subClassOf dbo:Country .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#Low_Risk_Area

:Low_Risk_Area rdf:type owl:Class ;
               
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hitBy ;
                                                            owl:someValuesFrom :Natural_Disaster
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty dbo:country ;
                                                            owl:onClass :Rich_Country ;
                                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                          ]
                                                        )
                                   ] ;
               
               rdfs:subClassOf :Area .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#Medium_Risk_Area

:Medium_Risk_Area rdf:type owl:Class ;
                  
                  rdfs:subClassOf :Area .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#Middle_Income_Country

:Middle_Income_Country rdf:type owl:Class ;
                       
                       owl:equivalentClass [ rdf:type owl:Class ;
                                             owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :GNI_per_capita ;
                                                                    owl:minQualifiedCardinality "1046"^^xsd:nonNegativeInteger ;
                                                                    owl:onDataRange xsd:integer
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :GNI_per_capita ;
                                                                    owl:maxQualifiedCardinality "12735"^^xsd:nonNegativeInteger ;
                                                                    owl:onDataRange xsd:integer
                                                                  ]
                                                                )
                                           ] ;
                       
                       rdfs:subClassOf dbo:Country .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#Natural_Disaster

:Natural_Disaster rdf:type owl:Class .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#No_Risk_Area

:No_Risk_Area rdf:type owl:Class ;
              
              rdfs:subClassOf :Area .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#Rich_Country

:Rich_Country rdf:type owl:Class ;
              
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :GNI_per_capita ;
                                    owl:minQualifiedCardinality "12736"^^xsd:nonNegativeInteger ;
                                    owl:onDataRange xsd:integer
                                  ] ;
              
              rdfs:subClassOf dbo:Country .



###  http://www.semanticweb.org/daan_julien/ontologies/natural_disaster_risk#Volcanic_Eruption

